{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "plugins": ["prettier"],
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": ["error"],
      "max-classes-per-file": "off",
      "no-console": "off"
    }
}
